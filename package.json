{
  "name": "qk-release",
  "version": "1.0.3",
  "description": "A beautiful javascript tool for quick release",
  "author": "YanPanMichael",
  "license": "MIT",
  "files": [
    "lib",
    "bin",
    "CHANGELOG.md"
  ],
  "bin": {
    "qkrelease": "bin/cli.js"
  },
  "scripts": {
    "changelog": "standard-version",
    "release": "node ./bin/cli.js --skipBuild",
    "format": "prettier --write .",
    "docs": "vitepress dev docs",
    "docs:build": "vitepress build docs"
  },
  "engines": {
    "node": ">=10.0.0"
  },
  "keywords": [
    "node",
    "release"
  ],
  "dependencies": {
    "execa": "^5.1.1",
    "fs": "^0.0.1-security",
    "minimist": "^1.2.5",
    "path": "^0.12.7",
    "picocolors": "^1.0.0",
    "prettier": "^2.5.1",
    "prompts": "^2.4.2",
    "standard-version": "^9.3.2"
  },
  "standard-version": {
    "skip": {
      "bump": true,
      "commit": true,
      "tag": true
    }
  }
}
